{
  "expo": {
    "sdkVersion": "32.0.0",
    "icon": "./assets/favicon.png",
    "name": "Cami y Aqui app",
    "slug": "cami-y-aqui",
    "privacy": "unlisted",
    "loading": {
      "hideExponentText": true,
      "backgroundImage": "./assets/splashscreen.png"
    },
    "orientation": "portrait",
    "primaryColor": "#cccccc",
    "packagerOpts": {
      "assetExts": [
        "ttf"
      ]
    },
    "androidStatusBarColor": "#334393",
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#334393"
    },
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.1",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.uetke.camiyaqui"
    },
    "description": "Our wedding App. You need to be registered on our website in order to use it.",
    "android": {
      "package": "com.uetke.camiyaqui"
    },
    "splash": {
      "image": "./assets/splashscreen.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "githubUrl": "https://github.com/aquilesC/cami-y-aqui-app",
    "notification": {
      "icon": "./assets/notification_icon.png",
      "color": "#000000",
      "androidMode": "collapse",
      "androidCollapsedTitle": "#{unread_notifications} new notifications"
    }
  }
}
